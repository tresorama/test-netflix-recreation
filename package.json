{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "author": "Jacopo Marrone",
  "license": "MIT",
  "scripts": {
    "dev:openbrowser": "npm-run-all -s open-browser dev",
    "dev": "next dev",
    "build": "rimraf .next && next build",
    "start": "next start",
    "lint": "npm-run-all -s lint:**",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "next lint",
    "test": "npm-run-all -s test:**",
    "test:jest": "jest",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "npm-run-all chakra-ui:generate-theme-typings",
    "chakra-ui:generate-theme-typings": "chakra-cli tokens ./src/lib/chakra-ui/chakra.theme.ts",
    "open-browser": "open http://localhost:3000 && exit 0",
    "deps:auto-upgrade-test": "npm update && npm run build && npm run start",
    "deps:auto-upgrade-save": "npm update --save"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-query": "^4.19.1",
    "@trpc/client": "^10.4.3",
    "@trpc/next": "^10.4.3",
    "@trpc/react-query": "^10.4.3",
    "@trpc/server": "^10.4.3",
    "autoprefixer": "^10.4.13",
    "framer-motion": "^7.6.18",
    "next": "^12.3.4",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.2.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^16.18.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-next": "^12.3.4",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  }
}
