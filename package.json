{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "author": "Jacopo Marrone",
  "license": "MIT",
  "scripts": {
    "dev:openbrowser": "npm-run-all -s open-browser dev",
    "dev": "next dev",
    "build": "rimraf .next && next build",
    "start": "next start",
    "lint": "npm-run-all -s lint:**",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "next lint",
    "test": "npm-run-all -s test:**",
    "test:jest": "jest",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "npm-run-all chakra-ui:generate-theme-typings",
    "chakra-ui:generate-theme-typings": "chakra-cli tokens ./src/lib/chakra-ui/chakra.custom-theme.ts",
    "open-browser": "open http://localhost:3000 && exit 0",
    "deps:auto-upgrade-test": "npm update && npm run build && npm run start",
    "deps:auto-upgrade-save": "npm update --save",
    "deps:reinstall": "rimraf ./node_modules && npm i"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "framer-motion": "^7.6.1",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.1.8",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "4.8.4"
  }
}
